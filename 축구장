import turtle as t

tb = t.Turtle()
tb.color("white")
tb.shape("circle")
tb.up()
tb.goto(0,0)

t.setup(600,600)
t.bgcolor("green")
t.speed(0)
t.up()
t.goto(-300,0)
t.down()
t.fd(600)
t.up()
t.goto(-50,-245)
t.down()
t.seth(270)
t.fd(30)
t.seth(0)
t.fd(100)
t.seth(90)
t.fd(30)
t.up()
t.goto(50,245)
t.down()
t.seth(90)
t.fd(30)
t.seth(180)
t.fd(100)
t.seth(270)
t.fd(30)
t.up()
t.goto(0,-40)
t.down()
t.seth(0)
t.circle(40)
t.up()
